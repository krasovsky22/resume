{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@": [
        "src/index.js",
        "src/index.jsx",
        "src/index.json",
        "src/index.ts",
        "src/index.tsx",
        "src/index.scss",
        "src/index.css"
      ],
      "@components/*": ["src/Components/*"],
      "@components": [
        "src/Components/index.js",
        "src/Components/index.jsx",
        "src/Components/index.json",
        "src/Components/index.ts",
        "src/Components/index.tsx",
        "src/Components/index.scss",
        "src/Components/index.css"
      ],
      "@fonts/*": ["src/Fonts/*"],
      "@fonts": [
        "src/Fonts/index.js",
        "src/Fonts/index.jsx",
        "src/Fonts/index.json",
        "src/Fonts/index.ts",
        "src/Fonts/index.tsx",
        "src/Fonts/index.scss",
        "src/Fonts/index.css"
      ]
    }
  }
}
